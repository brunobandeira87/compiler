LET INT main(INT mago, BOOL gordo, INT sangue) = VALOF {
        IF(a == 2){
                WRITEF(ALO);
                WHILE(TRUE){
                        CALL FUNC(a);
                }
        }
        RESULTIS 0;
}

AND BOOL teste(INT a, BOOL x) = VALOF {
        WHILE(a < 10){
                IF(x){
                        CALL main();
                }
        }
}
